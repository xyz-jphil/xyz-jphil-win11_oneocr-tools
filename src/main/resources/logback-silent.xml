<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <!-- Silent configuration - no output at all -->
    <statusListener class="ch.qos.logback.core.status.NopStatusListener" />
    
    <!-- Suppress PDFBox logging noise -->
    <logger name="org.apache.pdfbox" level="ERROR" />
    <logger name="org.apache.pdfbox.contentstream.PDFStreamEngine" level="OFF" />
    
    <!-- Root logger set to ERROR level to suppress all non-critical logging -->
    <root level="ERROR">
        <!-- No appenders = no output -->
    </root>
</configuration>